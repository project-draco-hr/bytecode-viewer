{
  ZipInputStream jis=new ZipInputStream(new FileInputStream(jarFile));
  ZipEntry entry;
  while ((entry=jis.getNextEntry()) != null) {
    try {
      final String name=entry.getName();
      if (!name.endsWith(".class")) {
        if (!entry.isDirectory())         BytecodeViewer.loadedResources.put(name,getBytes(jis));
      }
 else {
        byte[] bytes=getBytes(jis);
        String cafebabe=String.format("%02X",bytes[0]) + String.format("%02X",bytes[1]) + String.format("%02X",bytes[2])+ String.format("%02X",bytes[3]);
        if (cafebabe.toLowerCase().equals("cafebabe")) {
          try {
            final ClassNode cn=getNode(bytes);
            clazzList.put(cn.name,cn);
          }
 catch (          Exception e) {
            e.printStackTrace();
          }
        }
 else {
          System.out.println(jarFile + ">" + name+ ": Header does not start with CAFEBABE, ignoring.");
        }
      }
    }
 catch (    Exception e) {
      new the.bytecode.club.bytecodeviewer.api.ExceptionUI(e);
    }
 finally {
      jis.closeEntry();
    }
  }
  jis.close();
}
