{
  int minRating=0;
  if (sumLength <= FOUR) {
    minRating=FIVE;
  }
 else   if (sumLength >= FIVE && sumLength <= SEVEN) {
    minRating=FOUR;
  }
 else   if (sumLength >= EIGHT && sumLength <= ELEVEN) {
    minRating=THREE;
  }
 else   if (sumLength == TWELVE) {
    minRating=TWO;
  }
 else {
    minRating=ONE;
  }
  return minRating;
}
