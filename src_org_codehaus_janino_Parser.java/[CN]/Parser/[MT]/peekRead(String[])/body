{
  if (this.nextToken == null) {
    Token t=this.scanner.produce();
    int idx=Parser.indexOf(values,t.value);
    if (idx != -1)     return idx;
    this.nextToken=t;
    return -1;
  }
  int idx=Parser.indexOf(values,this.nextToken.value);
  if (idx == -1)   return -1;
  this.nextToken=this.nextButOneToken;
  this.nextButOneToken=null;
  return idx;
}
