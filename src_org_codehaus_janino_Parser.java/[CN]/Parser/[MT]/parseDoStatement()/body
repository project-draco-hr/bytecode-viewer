{
  final Location location=this.location();
  this.read("do");
  final Statement body=this.parseStatement();
  this.read("while");
  this.read("(");
  final Rvalue condition=this.parseExpression().toRvalueOrCompileException();
  this.read(")");
  this.read(";");
  return new DoStatement(location,body,condition);
}
