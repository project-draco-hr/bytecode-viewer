{
  try {
    sm.stopBlocking();
    ProcessBuilder pb=new ProcessBuilder("java","-version");
    Process p=pb.start();
    sm.setBlocking();
    if (p != null)     return "java";
  }
 catch (  Exception e) {
    sm.setBlocking();
    boolean empty=java.isEmpty();
    while (empty) {
      showMessage("You need to set your Java path, this requires the JRE to be downloaded." + BytecodeViewer.nl + "(C:/programfiles/Java/JRE_xx/bin/javac.exe)");
      viewer.java();
      if (!java.isEmpty())       empty=false;
    }
  }
  return java;
}
