{
  setPanes();
  if (pane2 == 1 && BytecodeViewer.viewer.panel2Proc_E.isSelected())   return true;
  if (pane2 == 2 && BytecodeViewer.viewer.panel2CFR_E.isSelected())   return true;
  if (pane2 == 3 && BytecodeViewer.viewer.panel2Fern_E.isSelected())   return true;
  if (pane2 == 6 && BytecodeViewer.viewer.panel2Smali_E.isSelected())   return true;
  if (pane2 == 9 && BytecodeViewer.viewer.panel2JDGUI_E.isSelected())   return true;
  if ((pane2 == 7 || pane2 == 8) && BytecodeViewer.viewer.panel2Krakatau_E.isSelected())   return true;
  return false;
}
