{
  final IClass[] pts=new IClass[arguments.length];
  for (int i=0; i < arguments.length; ++i)   pts[i]=this.getType(arguments[i]);
  return targetType.new IMethod(){
    @Override public String getName(){
      return name;
    }
    @Override public IClass getReturnType(){
      return IClass.INT;
    }
    @Override public boolean isStatic(){
      return false;
    }
    @Override public boolean isAbstract(){
      return false;
    }
    @Override public boolean isVarargs(){
      return false;
    }
    @Override public IClass[] getParameterTypes2(){
      return pts;
    }
    @Override public IClass[] getThrownExceptions2(){
      return new IClass[0];
    }
    @Override public Access getAccess(){
      return Access.PUBLIC;
    }
    @Override public Annotation[] getAnnotations(){
      return new Annotation[0];
    }
  }
;
}
