{
  if (name != null)   name=name.replace(oldName,newName);
  if (desc != null)   desc=desc.replace(oldName,newName);
  if (signature != null)   signature=signature.replace(oldName,newName);
  if (exceptions != null)   for (int i=0; i < exceptions.length; i++)   exceptions[i]=exceptions[i].replace(oldName,newName);
  return null;
}
