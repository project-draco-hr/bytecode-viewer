{
  if (sh == 0)   return "";
  StringBuilder res=new StringBuilder();
  for (int i=0; i < Mod.MAPPINGS.length; i+=2) {
    if ((sh & ((Short)Mod.MAPPINGS[i + 1]).shortValue()) == 0)     continue;
    if (res.length() > 0)     res.append(' ');
    res.append((String)Mod.MAPPINGS[i]);
  }
  return res.toString();
}
