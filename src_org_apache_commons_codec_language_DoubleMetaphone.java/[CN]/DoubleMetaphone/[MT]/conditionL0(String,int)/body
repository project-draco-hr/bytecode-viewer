{
  if (index == value.length() - 3 && contains(value,index - 1,4,"ILLO","ILLA","ALLE")) {
    return true;
  }
 else   if ((contains(value,value.length() - 2,2,"AS","OS") || contains(value,value.length() - 1,1,"A","O")) && contains(value,index - 1,4,"ALLE")) {
    return true;
  }
 else {
    return false;
  }
}
