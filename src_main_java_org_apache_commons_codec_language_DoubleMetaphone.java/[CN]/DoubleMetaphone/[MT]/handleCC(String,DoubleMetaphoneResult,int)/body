{
  if (contains(value,index + 2,1,"I","E","H") && !contains(value,index + 2,2,"HU")) {
    if ((index == 1 && charAt(value,index - 1) == 'A') || contains(value,index - 1,5,"UCCEE","UCCES")) {
      result.append("KS");
    }
 else {
      result.append('X');
    }
    index+=3;
  }
 else {
    result.append('K');
    index+=2;
  }
  return index;
}
