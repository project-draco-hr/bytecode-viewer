{
  if (isEmpty(raw)) {
    return EMPTY_BYTE_ARRAY;
  }
  final byte[] l_ascii=new byte[raw.length << 3];
  for (int ii=0, jj=l_ascii.length - 1; ii < raw.length; ii++, jj-=8) {
    for (int bits=0; bits < BITS.length; ++bits) {
      if ((raw[ii] & BITS[bits]) == 0) {
        l_ascii[jj - bits]='0';
      }
 else {
        l_ascii[jj - bits]='1';
      }
    }
  }
  return l_ascii;
}
