{
  if (args == null || args.length == 0)   return false;
  try {
    CommandLine cmd=parser.parse(options,args);
    if (cmd.hasOption("help") || cmd.hasOption("list") || cmd.hasOption("decompiler")|| cmd.hasOption("i")|| cmd.hasOption("o")|| cmd.hasOption("t")|| cmd.hasOption("nowait")) {
      return true;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return false;
}
